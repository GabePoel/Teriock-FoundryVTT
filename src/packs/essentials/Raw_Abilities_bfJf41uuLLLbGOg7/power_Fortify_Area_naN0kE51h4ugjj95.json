        "sustained": false,
        "range": "Self.",
        "overview": {
          "base": "You may spend a <a href=\"https://wiki.teriock.com/index.php/Core:Short_Rest\" title=\"Core:Short Rest\" target=\"_blank\">short rest</a> building defensive fortifications around the area you are resting in. You and your allies that are in this are when <a href=\"https://wiki.teriock.com/index.php/Core:Combat\" title=\"Core:Combat\" target=\"_blank\">combat</a> begins gain <span class=\"dice\" data-full-roll=\"1d10\" data-quick-roll=\"1d10\" data-type=\"gainTempHp\">[[/roll 1d10]]</span> <a href=\"https://wiki.teriock.com/index.php/Core:Temporary_Hit_Points\" title=\"Core:Temporary Hit Points\" target=\"_blank\">temporary HP</a> for the duration of that <a href=\"https://wiki.teriock.com/index.php/Core:Combat\" title=\"Core:Combat\" target=\"_blank\">combat</a>. This area counts as having been \"fortified\".",
          "proficient": "You and your allies instead gain <span class=\"dice\" data-full-roll=\"1d10 + (@p)d10\" data-quick-roll=\"1d10 + 1d10\" data-type=\"gainTempHp\">[[/roll 1d10 + (@p)d10]]</span> <a href=\"https://wiki.teriock.com/index.php/Core:Temporary_Hit_Points\" title=\"Core:Temporary Hit Points\" target=\"_blank\">temporary HP</a>.",
          "fluent": "You and your allies instead gain <span class=\"dice\" data-full-roll=\"1d10 + (@f)d10\" data-quick-roll=\"1d10 + 1d10\" data-type=\"gainTempHp\">[[/roll 1d10 + (@f)d10]]</span> <a href=\"https://wiki.teriock.com/index.php/Core:Temporary_Hit_Points\" title=\"Core:Temporary Hit Points\" target=\"_blank\">temporary HP</a>."
        },
