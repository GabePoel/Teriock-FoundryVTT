<div class="children-container">
  {{#each document.visibleTypes as |type|}}
    {{#if (or @root/editable (exists (lookup @root (path @root/TERIOCK.options.document type "getter"))))}}
      <div class="ab-main">
        {{#if @root/editable}}
          <button class="create{{ucFirst type}}" data-action="create{{ucFirst type}}">Add {{path
            @root/TERIOCK.options.document type "name"}}</button>{{/if}}
        {{#each (lookup @root (path @root/TERIOCK.options.document type "getter")) as |doc|}}
          {{> (template "ui-templates/block.hbs") (merge doc.embedParts (object
            relative=@root/document.uuid))}}
        {{/each}}
      </div>
    {{/if}}
  {{/each}}
</div>
