<div class="{{cssClass}} tsheet-page">
  {{#if (eq tab "overview")}}
    <div class="ab-sheet-everything">
      <div class="ab-sheet-top">
        {{> (template "document-templates/shared/header.hbs")}}
        <div class="ab-bars">
          {{> (template "document-templates/effect-templates/property-template/bars/pr-status-bar.hbs")}}
        </div>
      </div>
      {{> (template "document-templates/effect-templates/property-template/parts/pr-menu.hbs")}}
      <div class="ab-main">
        <div class="ab-tags-all">
          {{> (template "document-templates/effect-templates/property-template/tags/pr-tags.hbs")}}
        </div>
        {{> (template "document-templates/shared/display-fields.hbs")}}
      </div>
      {{#if (or editable (exists subProperties))}}
        <div class="ab-main">
          {{#if editable}}
            <button class="createProperty" data-action="createProperty">Add Child Property</button>{{/if}}
          {{#each subProperties as |doc|}}
            {{> (template "ui-templates/block.hbs") (merge doc.embedParts (object
              relative=../document.uuid))}}
          {{/each}}
        </div>
      {{/if}}
    </div>
  {{/if}}
  {{#if (eq tab "impacts")}}
    {{> (template "document-templates/effect-templates/property-template/parts/pr-impacts.hbs")}}
  {{/if}}
</div>
