{
  "plugins": [
    "./plugins/custom-tags.js"
  ],
  "source": {
    "include": [
      "./src/module/"
    ],
    "includePattern": "\\.(mjs|ts)$"
  },
  "sourceType": "module",
  "opts": {
    "destination": "docs",
    "recurse": true
  },
  "tags": {
    "allowUnknownTags": true,
    "dictionaries": [
      "jsdoc",
      "closure"
    ]
  }
}