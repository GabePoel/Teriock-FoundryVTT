<div class="{{cssClass}} character-page" autocomplete="off">
    {{> 'systems/teriock/templates/parts/character-parts/character-sidebar.hbs' }}
    <div class="character-tabbers">
        <div class="character-tabber ui-controls {{tabActive system.sheet.activeTab "overview"}}" tab="overview">
            <i class="fa-solid fa-person"></i>
        </div>
        <div class="character-tabber ui-controls {{tabActive system.sheet.activeTab "tradecrafts"}}" tab="tradecrafts">
            <i class="fa-solid fa-compass-drafting"></i>
        </div>
        <div class="character-tabber ui-controls {{tabActive system.sheet.activeTab "abilities"}}" tab="abilities">
            <i class="fa-solid fa-hand-sparkles"></i>
        </div>
        <div class="character-tabber ui-controls {{tabActive system.sheet.activeTab "classes"}}" tab="classes">
            <i class="fa-solid fa-hat-wizard"></i>
        </div>
        <div class="character-tabber ui-controls {{tabActive system.sheet.activeTab "powers"}}" tab="powers">
            <i class="fa-solid fa-award"></i>
        </div>
        <div class="character-tabber ui-controls {{tabActive system.sheet.activeTab "inventory"}}" tab="inventory">
            <i class="fa-solid fa-treasure-chest"></i>
        </div>
        <div class="character-tabber ui-controls {{tabActive system.sheet.activeTab "notes"}}" tab="notes">
            <i class="fa-solid fa-pen-fancy"></i>
        </div>
    </div>
    <div class="character-tab-content">
        {{#if (eq system.sheet.activeTab "overview")}}
            {{> 'systems/teriock/templates/parts/character-parts/tabs/character-tab-overview.hbs' }}
        {{/if}}
        {{#if (eq system.sheet.activeTab "tradecrafts")}}
            {{> 'systems/teriock/templates/parts/character-parts/tabs/character-tab-tradecrafts.hbs' }}
        {{/if}}
        {{#if (eq system.sheet.activeTab "abilities")}}
            {{> 'systems/teriock/templates/parts/character-parts/tabs/character-tab-abilities.hbs' }}
        {{/if}}
        {{#if (eq system.sheet.activeTab "classes")}}
            {{> 'systems/teriock/templates/parts/character-parts/tabs/character-tab-classes.hbs' }}
        {{/if}}
        {{#if (eq system.sheet.activeTab "powers")}}
            {{> 'systems/teriock/templates/parts/character-parts/tabs/character-tab-powers.hbs' }}
        {{/if}}
        {{#if (eq system.sheet.activeTab "inventory")}}
            {{> 'systems/teriock/templates/parts/character-parts/tabs/character-tab-inventory.hbs' }}
        {{/if}}
        {{#if (eq system.sheet.activeTab "notes")}}
            {{> 'systems/teriock/templates/parts/character-parts/tabs/character-tab-notes.hbs' }}
        {{/if}}
</div>