<div class="ab-header">
  <link rel="preconnect" href="https://fonts.googleapis.com" />
  <link rel="preconnect" href="https://fonts.gstatic.com" crossorigin />
  <link
    href="https://fonts.googleapis.com/css2?family=Cinzel:wght@400..900&display=swap"
    rel="stylesheet"
  />
  <div class="ab-profile-img">
    <img
      class="profile-img"
      src="{{img}}"
      data-edit="img"
      data-action="onEditImage"
      title="{{name}}"
    />
  </div>
  <div class="ab-title">
    <input type="text" name="name" value="{{name}}" placeholder="Name" />
  </div>
  <div class="ab-top-buttons-container">
    <div class="ab-top-buttons">
      <div class="ab-icon ab-top-button open-button">
        {{{config.icons.wiki}}}
      </div>
      <div class="ab-icon ab-top-button reload-button">
        {{{config.icons.import}}}
      </div>
      <div class="ab-icon ab-top-button chat-button">
        {{{config.icons.chat}}}
      </div>
    </div>
  </div>
</div>