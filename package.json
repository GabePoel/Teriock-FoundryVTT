{
  "name": "teriock",
  "version": "2.0.0",
  "description": "CSS compiler for the Teriock system. Based on the systemless one by Asacolips.",
  "scripts": {
    "build": "sass style/teriock.scss css/teriock.css --style=expanded",
    "watch": "sass style/teriock.scss css/teriock.css --style=expanded --source-map --watch",
    "createSymlinks": "node ./tools/create-symlinks.mjs"
  },
  "browserslist": [
    "last 3 versions"
  ],
  "author": "Gabriel",
  "license": "MIT",
  "private": true,
  "devDependencies": {
    "sass": "^1.53.0"
  },
  "dependencies": {
    "js-yaml": "^4.1.0",
    "jsdom": "^26.1.0"
  }
}
